{
  "name": "backend",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "npm run db:generate-schema && prisma generate && tsc",
    "start": "prisma generate && node dist/index.js",
    "postinstall": "node scripts/postinstall.js",
    "db:generate-schema": "node scripts/generate-schema.js",
    "db:setup": "npm run db:generate-schema && prisma generate && prisma db push",
    "db:migrate": "npm run db:generate-schema && prisma generate && prisma migrate deploy",
    "db:reset": "npm run db:generate-schema && prisma db push --force-reset",
    "fetch-races": "ts-node src/scripts/fetchRaces.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "cheerio": "^1.1.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "iconv-lite": "^0.7.2",
    "nanoid": "^3.3.11",
    "prisma": "^5.22.0"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.35",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/node": "^25.0.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3"
  }
}
